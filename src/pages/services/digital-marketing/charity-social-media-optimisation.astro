              </div>
            </div>
            <!-- Single Widget -->
            <div class="single-widget widget-brochure">
              <h4 class="widget-title">Brochure</h4>
              <ul>
                <li>
                  <a href="#"
                    ><i class="fas fa-file-pdf"></i> Download Brochure
                  </a>
                </li>
                <li>
                  <a href="#"
                    ><i class="fas fa-file-pdf"></i> Company Details
                  </a>
                </li>
              </ul>
            </div>
            <!-- End Single Widget -->
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- End Services Details Area -->
</Layout>
