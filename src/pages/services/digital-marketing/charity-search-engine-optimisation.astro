        .slider-heading {
            color: #5c82ff;
        }
        .swiper {
            width: 100%;
            height: 100%;
        }

        .swiper-slide {
            text-align: center;
            font-size: 18px;
            background: #fff;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            border-radius: 200px;
        }

        .swiper-slide img {
            display: block;
            width: 100%;
            height: 100%;
