            border: white 1px solid;
        }
        .email-section-mobile {
            background-image: linear-gradient(
                to left,
                rgba(0, 0, 0, 0.329),
                white
            );
            height: 50px;
            border-radius: 10px;
            /* padding: 5px 5px 5px 5px; */
            border: white 1px solid;
        }

        .email-section input {
            background-color: transparent;
            border: none;
        }
        .email-section-mobile input {
            background-color: transparent;
            border: none;
        }
        .email-section-mobile button {
