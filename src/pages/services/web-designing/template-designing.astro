---
import Layout from "../../../layouts/Layout.astro";
import { SEO } from "astro-seo";
import ourteamList from "../../../../public/assets/js/developers-list";
import SliderNew from "../../../components/SliderNew.astro";
---

<SEO
    title="Template Designing - Techno Serve Solutions Agency 2024"
    description="A heavily optimized description full of well-researched keywords."
    openGraph={{
        basic: {
            title: "A Very Descriptive Title",
            type: "A type.",
            image: "https://user-images.githubusercontent.com/5182256/131216951-8f74f425-f775-463d-a11b-0e01ad9fce8d.png",
        },
    }}
    twitter={{
        creator: "@jonasmerlins1",
    }}
    extend={{
        // extending the default link tags
        link: [{ rel: "icon", href: "/favicon.ico" }],
        // extending the default meta tags
        meta: [
            {
                name: "twitter:image",
                content:
                    "https://user-images.githubusercontent.com/5182256/131216951-8f74f425-f775-463d-a11b-0e01ad9fce8d.png",
            },
            { name: "twitter:title", content: "Tinker Tailor Soldier Spy" },
            { name: "twitter:description", content: "Agent" },
        ],
    }}
/>

<Layout>
    <!-- Start Breadcrumb 
    ============================================= -->
    <div
        class="breadcrumb-area bg-gray bg-cover"
        style="background-image: url(/assets/img/shape/banner-3.jpg);"
    >
        <div class="container">
            <div class="breadcrumb-item">
                <div class="breadcrum-shape">
                    <img src="/assets/img/shape/16.png" alt="Image Not Found" />
                </div>
                <div class="row">
                    <div class="col-lg-8">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li>
                                    <a href="/"
                                        ><i class="fas fa-home"></i> Home</a
                                    >
                                </li>
                                <li><a href="/services">Services</a></li>
                                <li class="active">Template Designing</li>
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Breadcrumb -->

    <!-- Star Services Details Area
    ============================================= -->
    <!-- Section One Start -->
    <div
        class="section-one container d-md-flex col-12 my-2 my-md-3 align-items-center"
    >
        <div class="col-12 col-md-6">
            <h1 class="fw-bold text-black display-4">
                Customized Template Design Services
            </h1>
        </div>
        <div class="col-12 col-md-6 px-md-5 mx-md-5">
            <p class="text-black our-font mb-md-4 pe-2 px-md-0 mx-md-0">
                Don’t worry, enhance your brand with our Custom Template
                Designing. Expertly crafted, our templates blend creativity and
                precision to make your nonprofit stand out towards the
                donations. We tailor to meet your unique needs as we offer a
                perfect blend of creativity and functionality. Let's bring your
                vision to life helping the needy!
            </p>
        </div>
    </div>
    <!-- Section one End -->

    <!-- Image section Start Here -->
    <div class="container mb-5">
        <img
            src="/assets/img/service/website-development2.png"
            alt="Website Development"
            class="img-fluid"
        />
    </div>
    <!-- Image section End Here -->

    <!-- Section Two Start -->
    <div class="section-two container d-md-flex align-items-center col-12 my-5">
        <div class="col-12 col-md-7 pe-2 me-2 pe-md-5 me-md-5">
            <h2 class="fw-bold text-black display-6">
                Platforms We Use for Template Designing
            </h2>
            <p class="text-black mb-md-4 pe-2 px-md-0 mx-md-0 our-font">
                Techno Serve aims to deliver you customized solutions on
                platforms like WordPress and React. Whether you need a dynamic
                WordPress theme or a responsive React component, our experts are
                here to enhance functionality and visual appeal.<br /><br /> We focus
                on creating templates that align with your charity identity, ensuring
                a seamless user experience and a standout presence online. Let us
                help you bring your vision to life with precision and style.
            </p>
            <div class="d-md-flex mb-4 text-start">
                <div class="col-12 col-md-7 px-md-0 px-2">
                    <ul class="list-group">
                        <li>
                            <i class="fas fa-check list-group-content"></i>
                            <span class="fw-bold"
                                >&nbsp;&nbsp;Websites Design</span
                            >
                        </li>
                        <li>
                            <i class="fas fa-check list-group-content"></i><span
                                class="fw-bold"
                            >
                                &nbsp;&nbsp;Branding Design</span
                            >
                        </li>
                        <li>
                            <i class="fas fa-check list-group-content"></i>
                            <span class="fw-bold"
                                >&nbsp;&nbsp;Responsive Website Design</span
                            >
                        </li>
                    </ul>
                </div>
                <div class="col-12 col-md-7 px-md-0 px-2">
                    <ul class="list-group">
                        <li>
                            <i class="fas fa-check list-group-content"></i><span
                                class="fw-bold"
                            >
                                &nbsp;Professional Design
                            </span>
                        </li>
                        <li>
                            <i class="fas fa-check list-group-content"></i><span
                                class="fw-bold"
                                >&nbsp;&nbsp;WCharity / Nonprofit Design
                            </span>
                        </li>
                        <li>
                            <i class="fas fa-check list-group-content"></i><span
                                class="fw-bold"
                                >&nbsp;&nbsp;Advertising design
                            </span>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="col-12 col-md-5">
            <img
                src="/assets/img/service/why-choose-us.png"
                alt="Why Choose Us"
                class="img-fluid"
            />
        </div>
    </div>

    <!-- Section Two End -->

    <!-- Section Three Start -->
    <div class="section-three">
        <div class="row container h-auto py-2 py-md-5">
            <div class="container content col-12 col-md-9 mx-md-5 mt-3 mt-md-0">
                <h2 class="slider-heading our-heading fw-bold">
                    Custom Template Design Case Studies
                </h2>
                <p class="text-white our-font">
                    Our successful template designed for charities in the UK is
                    in enhancing online visibility, attracting visitors, and
                    boosting donations. Look at the samples provided below:
                </p>
            </div>

            <div class="blank col-3 d-none d-md-block"></div>
        </div>

        <div class="slider-section pb-5">
            <SliderNew />
        </div>
    </div>
    <!-- Section Three End -->

    <!-- Section Four Start -->
    <div
        class="section-four container d-md-flex align-items-center col-12 my-5"
    >
        <div class="container col-12 col-md-4">
            <img
                src="/assets/img/service/mobile-first-opti.png"
                alt="Mobile First Optimization"
                class="img-fluid"
            />
        </div>
        <div class="col-12 col-md-8 px-md-5 mx-md-5 px-2 mt-4 mt-md-0">
            <h2 class="fw-bold text-black display-6">
                Benefits of Custom Template Design for Nonprofits.
            </h2>
            <p class="text-black our-font">
                Unlock the potential of your nonprofit with our customized
                template design services. We will create designs specifically to
                meet your unique needs. <br /><br />With over 10 years of
                experience in charity, we focus on clarity and impact, we ensure
                your message resonates with the donors, helping you achieve your
                mission more effectively. Trust us to deliver designs that are
                both visually appealing and strategically aligned with your
                goals, allowing you to focus on what truly matters making a
                difference
            </p>
            <div class="d-flex justify-content-center justify-content-md-start">
                <button class="btn section-four-btn mt-4 mx-md-0"
                    ><a
                        href="https://technoservesolutions.com/services/web-designing/landingpage-designing/"
                        class="text-white">Landing Page</a
                    ><i class="fas fa-arrow-right"></i></button
                >
            </div>
        </div>
    </div>
    <!-- Section Four End -->

    <!-- Section Five Start -->
    <div class="section-two container d-md-flex align-items-center col-12">
        <div class="col-12 col-md-7 me-md-5 pe-md-5">
            <h2 class="fw-bold text-black display-6">
                Our Team of Custom Template Design
            </h2>
            <p class="text-black mb-2 pe-2 me-md-5 pe-md-5 mb-md-4 our-font">
                We’re dedicated to expanding your charity globally with our WP
                development expertise. With over 10 years of experience, our
                experts are equipped to elevate your nonprofit to the next
                level.
            </p>
            <div class="d-flex justify-content-center justify-content-md-start">
                <button class="btn section-four-btn mt-4 mx-md-0"
                    ><a
                        href="https://technoservesolutions.com/contact/"
                        class="text-white">Contact Us</a
                    ><i class="fas fa-arrow-right"></i></button
                >
            </div>
        </div>
        <div class="col-12 col-md-5 mt-4 mt-md-0">
            <div class="profile-grid">
                <div class="profile-item">
                    <img
                        src="/assets/img/service/OurTeam/anwar.png"
                        alt="anwar"
                    />
                </div>
                <div class="profile-item">
                    <img
                        src="/assets/img/service/OurTeam/mubin.png"
                        alt="mubin"
                    />
                </div>
                <div class="profile-item">
                    <img
                        src="/assets/img/service/OurTeam/sahil.png"
                        alt="sahil"
                    />
                </div>
                <div class="profile-item">
                    <img
                        src="/assets/img/service/OurTeam/hussain.png"
                        alt="hussain"
                    />
                </div>
                <div class="profile-item">
                    <img
                        src="/assets/img/service/OurTeam/samshad.png"
                        alt="samshad"
                    />
                </div>
                <div class="profile-item">
                    <img
                        src="/assets/img/service/OurTeam/kaseem.png"
                        alt="kaseem"
                    />
                </div>
                <div class="profile-item">
                    <img
                        src="/assets/img/service/OurTeam/ayann.png"
                        alt="ayann"
                    />
                </div>
                <div class="profile-item">
                    <img
                        src="/assets/img/service/OurTeam/danial.png"
                        alt="danial"
                    />
                </div>
                <div class="profile-item">
                    <img
                        src="/assets/img/service/OurTeam/ahmed.png"
                        alt="ahmed"
                    />
                </div>
            </div>
        </div>
    </div>

    <!-- Section Five End -->

    <!-- Section Six Faq Section Start -->
    <div class="section-six container">
        <div class="faq-style-one service-faq mt-40">
            <h2 class="mb-30 fw-bold"><b>Frequently Asked Questions</b></h2>
            <div class="accordion mt--20" id="faqAccordion">
                <div class="accordion-item">
                    <h3 class="accordion-header" id="headingOne">
                        <button
                            class="accordion-button"
                            type="button"
                            data-bs-toggle="collapse"
                            data-bs-target="#collapseOne"
                            aria-expanded="true"
                            aria-controls="collapseOne"
                        >
                            What is custom template design for charities?
                        </button>
                    </h3>
                    <div
                        id="collapseOne"
                        class="accordion-collapse collapse show"
                        aria-labelledby="headingOne"
                        data-bs-parent="#faqAccordion"
                    >
                        <div class="accordion-body">
                            <p class="our-font">
                                Custom template design for charities involves
                                creating tailored website and branding templates
                                specifically for nonprofit organizations. These
                                templates are designed to reflect the charity’s
                                identity, enhance user experience, and improve
                                online visibility to boost donations and
                                support.
                            </p>
                        </div>
                    </div>
                </div>
                <div class="accordion-item">
                    <h3 class="accordion-header" id="headingTwo">
                        <button
                            class="accordion-button collapsed"
                            type="button"
                            data-bs-toggle="collapse"
                            data-bs-target="#collapseTwo"
                            aria-expanded="false"
                            aria-controls="collapseTwo"
                        >
                            How can custom templates benefit my nonprofit
                            organization?
                        </button>
                    </h3>
                    <div
                        id="collapseTwo"
                        class="accordion-collapse collapse"
                        aria-labelledby="headingTwo"
                        data-bs-parent="#faqAccordion"
                    >
                        <div class="accordion-body">
                            <p class="our-font">
                                Custom templates can greatly benefit your
                                nonprofit by improving your online presence,
                                attracting more visitors, and increasing
                                donations. They provide a unique and
                                professional look that aligns with your
                                charity’s mission and values, helping you
                                communicate your message effectively.
                            </p>
                        </div>
                    </div>
                </div>
                <div class="accordion-item">
                    <h3 class="accordion-header" id="headingThree">
                        <button
                            class="accordion-button collapsed"
                            type="button"
                            data-bs-toggle="collapse"
                            data-bs-target="#collapseThree"
                            aria-expanded="false"
                            aria-controls="collapseThree"
                        >
                            What platforms do you use for template design?
                        </button>
                    </h3>
                    <div
                        id="collapseThree"
                        class="accordion-collapse collapse"
                        aria-labelledby="headingThree"
                        data-bs-parent="#faqAccordion"
                    >
                        <div class="accordion-body">
                            <p class="our-font">
                                We design custom templates using platforms such
                                as WordPress and React. Whether you need a
                                dynamic WordPress theme or a responsive React
                                component, our team is equipped to deliver
                                high-quality solutions that enhance
                                functionality and visual appeal.
                            </p>
                        </div>
                    </div>
                </div>
                <div class="accordion-item">
                    <h3 class="accordion-header" id="headingFour">
                        <button
                            class="accordion-button collapsed"
                            type="button"
                            data-bs-toggle="collapse"
                            data-bs-target="#collapseFour"
                            aria-expanded="false"
                            aria-controls="collapseFour"
                        >
                            What types of design services do you offer?
                        </button>
                    </h3>
                    <div
                        id="collapseFour"
                        class="accordion-collapse collapse"
                        aria-labelledby="headingFour"
                        data-bs-parent="#faqAccordion"
                    >
                        <div class="accordion-body">
                            <p class="our-font">
                                We offer a range of design services including
                                website design, branding design, responsive
                                website design, professional design,
                                charity/nonprofit design, and advertising
                                design. Our focus is on creating designs that
                                are both visually appealing and strategically
                                effective.
                            </p>
                        </div>
                    </div>
                </div>
                <div class="accordion-item mb-5">
                    <h3 class="accordion-header" id="headingFive">
                        <button
                            class="accordion-button collapsed"
                            type="button"
                            data-bs-toggle="collapse"
                            data-bs-target="#collapseFive"
                            aria-expanded="false"
                            aria-controls="collapseFive"
                        >
                            How does your team ensure the templates meet our
                            unique needs?
                        </button>
                    </h3>
                    <div
                        id="collapseFive"
                        class="accordion-collapse collapse"
                        aria-labelledby="headingFive"
                        data-bs-parent="#faqAccordion"
                    >
                        <div class="accordion-body">
                            <p class="our-font">
                                Our team works closely with you to understand
                                your charity’s goals, identity, and
                                requirements. With over 10 years of experience,
                                we tailor each design to reflect your vision,
                                ensuring a seamless user experience and a strong
                                online presence that resonates with your target
                                audience.
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Section Six Faq Section End -->

    <!-- Section Seven Start -->
    <div class="container d-none d-md-block">
        <div
            class="section-seven h-100 rounded-3 d-md-flex align-items-center p-5 mb-5"
        >
            <div class="content col-12 col-md-6">
                <h2 class="text-white display-6 fw-bold">
                    Tell us about your project
                </h2>
                <p class="text-white">
                    To find out more about our services and how we can <br /> help
                    your business, please get in touch.
                </p>
            </div>
            <div class="col-12 col-md-6">
                <div class="d-md-flex email-section">
                    <input
                        type="email"
                        placeholder="Enter your email address"
                        class="form-control"
                    />
                    <button class="seven-btn">Submit</button>
                </div>
            </div>
        </div>
    </div>

    <!-- for mobile  -->
    <div class="container section-seven-mobile d-md-none">
        <div class="content">
            <h2 class="text-white display-6 fw-bold">
                Tell us about your project
            </h2>
            <p class="text-white">
                To find out more about our services and how we can help your
                business, please get in touch.
            </p>
            <div class="d-flex col-12 email-section-mobile mt-5">
                <input
                    type="email"
                    placeholder="Enter your email address"
                    class="form-control"
                />
                <button>Submit</button>
            </div>
        </div>
    </div>

    <!-- Section Seven End -->

    <!-- End Services Details Area -->

    <style>
        .breadcrumb-area {
            /* margin-top: -50px; */
        }
        .breadcrumb-item {
            /* margin-top: -50px; */
        }
        .section-three {
            background-color: #282828;
        }
        .slider-heading {
            color: #5c82ff;
        }
        .swiper {
            width: 100%;
            height: 100%;
        }

        .swiper-slide {
            text-align: center;
            font-size: 18px;
            background: #fff;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            border-radius: 200px;
        }

        .swiper-slide img {
            display: block;
            width: 100%;
            height: 100%;
        }

        .swiper-slide {
            width: 60%;
        }
        .swiper-slide:nth-child(2n) {
            /* width: 60%; */
        }

        .swiper-slide:nth-child(3n) {
            /* width: 40%; */
        }
        .slider-section {
            height: 80vh;
        }
        .our-heading {
            font-size: 1.75rem;
        }
        .our-font {
            font-size: 1.25rem;
        }
        @media (max-width: 768px) {
            .slider-section {
                height: 56vh;
            }
            .our-font {
                font-size: 1rem;
                color: black;
            }
            .our-heading {
                font-size: 0.9rem;
            }
        }
        .slider-section .swiper-content {
            background-color: #282828;
        }
        .section-four-btn {
            background-color: #8e66ff !important;
        }
        .section-seven {
            background-image: url("/assets/img/service/article-section.png");
            background-size: contain;
            background-repeat: no-repeat;
            background-position: center;
        }
        .section-seven-mobile {
            width: 90%;
            height: 500px;
            background-image: url("/assets/img/service/article-section-mobile.png");
            background-size: cover;
            background-repeat: no-repeat;
            background-position: center;
            border-radius: 13px;
            margin-bottom: 2rem;
            padding: 100px 20px;
        }
        .email-section {
            background-image: linear-gradient(
                to left,
                rgba(0, 0, 0, 0.329),
                white
            );
            border-radius: 5px;
            padding: 5px 5px 5px 5px;
            border: white 1px solid;
        }
        .email-section-mobile {
            background-image: linear-gradient(
                to left,
                rgba(0, 0, 0, 0.329),
                white
            );
            height: 50px;
            border-radius: 10px;
            /* padding: 5px 5px 5px 5px; */
            border: white 1px solid;
        }

        .email-section input {
            background-color: transparent;
            border: none;
        }
        .email-section-mobile input {
            background-color: transparent;
            border: none;
        }
        .email-section-mobile button {
            background-color: black;
            color: white;
            border: none;
            font-size: 1rem;
            padding: 0px 20px 0px 20px;
            padding-right: 40px;
            margin: 5px;
            border-radius: 8px;
            cursor: pointer;
        }
        .email-section input:focus input:active {
            background-color: transparent;
            border: none;
        }
        .email-section input::placeholder {
            color: #4e4e4e;
            font-size: 1.3rem;
        }
        .email-section-mobile input::placeholder {
            color: #4e4e4e;
            font-size: 0.9rem;
        }
        .seven-btn {
            background-color: black;
            color: white;
            border: none;
            /* padding: 10px 20px;  */
            margin-right: 5px;
            border-radius: 5px;
            cursor: pointer;
        }
        .seven-btn:hover {
            background-color: #8e66ff !important;
        }
        .seven-btn:focus {
            background-color: #8e66ff !important;
        }
        .list-group-content {
            color: #8e66ff !important;
        }

        .profile-grid {
            display: grid;
            grid-template-columns: repeat(3, 1fr);
            gap: 10px; /* Adjust the gap between items */
            padding: 20px; /* Optional, to add some padding around the grid */
            max-width: 900px; /* Adjust width as needed */
            margin: 0 auto; /* Center the grid horizontally */
        }

        .profile-item {
            background-color: #000;
            border-radius: 15px;
            overflow: hidden;
            border: 2px solid #000; /* To match the rounded corners and frame */
        }

        .profile-item img {
            display: block;
            width: 100%;
            height: auto;
        }
    </style>

    <script
        is:inline
        src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"
    ></script>

    <script is:inline>
        var Swiper = new Swiper(".mySwiper", {
            slidesPerView: "auto",
            spaceBetween: 50,
            pagination: {
                el: ".swiper-pagination",
                clickable: true,
            },
        });
    </script>
</Layout>
