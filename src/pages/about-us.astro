---
import Layout from "../layouts/Layout.astro";
---

<Layout title="Know More About Techno Serve Solutions Agency 2024">
    <!-- Start Breadcrumb 
    ============================================= -->
    <div
        class="breadcrumb-area bg-gray bg-cover"
        style="background-image: url(/assets/img/shape/banner-3.jpg);"
    >
        <div class="container">
            <div class="breadcrumb-item">
                <div class="breadcrum-shape">
                    <img src="/assets/img/shape/16.png" alt="Image Not Found" />
                </div>
                <div class="row">
                    <div class="col-lg-8">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                <li>
                                    <a href="#"
                                        ><i class="fas fa-home"></i> Home</a>
                                </li>
                                <li class="active">About</li>
                            </ol>
                        </nav>
                        <h1>About Us</h1>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Breadcrumb -->

    <!-- Start About 
    ============================================= -->
    <div class="about-style-two-area default-padding">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 about-style-two">
                    <div class="about-two-thumb">
                        <img
                            src="/assets/img/hero2.jpg"
                            alt="Image Not Found"
                        />
                        <div class="experience">
                            <h2><strong>09+</strong> Years Experience</h2>
                        </div>
                    </div>
                </div>
                <div
                    class="col-lg-6 about-style-two pl-50 pl-md-15 pl-xs-15 mt-60 mt-xs-40"
                >
                    <div class="about-two-info">
                        <h4 class="sub-title">About our compnay</h4>
                        <h2 class="title">
                            A Trusted HMRC-Listed Software Provider for
                            Charities and Non-Profits
                        </h2>
                        <p>
                            Techno Serve Solutions is a trusted digital
                            marketing agency listed on the HMRC Charities Online
                            service website <a href="https://www.gov.uk/"
                                >(www.gov.uk).</a
                            ><br /><br />
                            Our digital marketing agency is recognized by HMRC, ensuring
                            compliance and reliability. This listing highlights our
                            commitment to trustworthy and reliable services for charities
                            and non-profits. With Techno Serve, you can trust our
                            credibility and authenticity.<br /><br />
                            Contact us today to learn how Techno Serve can potentially
                            grow your islamic Non-Profit organization and help you
                            make a meaningful impact. <br />
                            Techno Serve Ltd Company Number: <strong
                                >08655298</strong
                            >
                        </p>
                        <div class="about-grid-info">
                            <a class="btn-round-animation" href="/service"
                                >Discover More <i class="fas fa-arrow-right"
                                ></i></a
                            >
                            <ul class="list-info-item">
                                <li>
                                    <h4>
                                        <a href="/service#designing"
                                            >Design <i
                                                class="fas fa-angle-right"
                                            ></i></a
                                        >
                                    </h4>
                                </li>
                                <li>
                                    <h4>
                                        <a href="/service#digital-marketing"
                                            >Digital Solution <i
                                                class="fas fa-angle-right"
                                            ></i></a
                                        >
                                    </h4>
                                </li>
                                <li>
                                    <h4>
                                        <a href="/service"
                                            >Strategy <i
                                                class="fas fa-angle-right"
                                            ></i></a
                                        >
                                    </h4>
                                </li>
                                <li>
                                    <h4>
                                        <a href="/service#digital-marketing"
                                            >Branding <i
                                                class="fas fa-angle-right"
                                            ></i></a
                                        >
                                    </h4>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
